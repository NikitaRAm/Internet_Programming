<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>zad1_servlet</servlet-name>
        <servlet-class>zad1_servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>zad1_servlet</servlet-name>
        <url-pattern>/zad1_servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>zad2_servlet</servlet-name>
        <servlet-class>zad2_servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>zad2_servlet</servlet-name>
        <url-pattern>/zad2_servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>zad3_servlet</servlet-name>
        <servlet-class>zad3_servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>zad3_servlet</servlet-name>
        <url-pattern>/zad3_servlet</url-pattern>
    </servlet-mapping>
</web-app>